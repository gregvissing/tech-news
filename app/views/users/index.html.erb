<h1>All htmlTechNews Users</h1>
<ul>
    <% @users.each do |user| %>
        <li><%= link_to user.username, user_path(user) %></li>
        <% user.bytes.each do |byte| %>
            <!-- above is adding all the bytes for each user under their username -->
            <p><%= byte.content %></p>
        <% end %>
    <% end %>
</ul>